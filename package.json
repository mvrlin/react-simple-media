{
  "name": "react-superq",
  "version": "1.0.0",
  "description": "Easy-to-use media queries for your React project",
  "keywords": [
    "react",
    "module",
    "viewport",
    "react qut",
    "react query",
    "mq",
    "media",
    "query",
    "queries",
    "media query",
    "media queries",
    "viewport",
    "plugins"
  ],
  "repository": "git@github.com:mvrlin/react-superq.git",
  "license": "MIT",
  "author": {
    "name": "Igor Talpa (mvrlin)",
    "email": "mvrlin@pm.me"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/*",
    "prepare": "rsync -a --delete src/* test/fixture/next/react-superq",
    "release": "npm run build && standard-version && git push --follow-tags && npm publish",
    "test": "vitest test/unit",
    "test:next": "npm run --prefix test/fixture/next dev",
    "test:ui": "vitest test/unit --ui",
    "test:vite": "npm run --prefix test/fixture/vite dev"
  },
  "dependencies": {
    "react": "18.2.0",
    "ua-parser-js": "1.0.2"
  },
  "devDependencies": {
    "@types/node": "18.7.11",
    "@types/react": "18.0.17",
    "@types/react-test-renderer": "18.0.0",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@vitest/ui": "0.22.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "happy-dom": "6.0.4",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "standard-version": "9.5.0",
    "tsup": "6.2.2",
    "typescript": "4.7.4",
    "vitest": "0.22.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
